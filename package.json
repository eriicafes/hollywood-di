{
  "name": "hollywood-di",
  "version": "0.5.2",
  "description": "ðŸ“¦ Type safe Dependency Injection for Typescript with zero decorators",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit && shx rm -rf dist && tsup src/index.ts --format esm,cjs --dts",
    "release": "pnpm run build && changeset publish",
    "watch": "vitest",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "Hollywood",
    "DI",
    "dependency injection",
    "IoC",
    "inversion of control",
    "container",
    "typescript"
  ],
  "author": "Eric Afes <eriicafes@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eriicafes/hollywood-di.git"
  },
  "bugs": {
    "url": "https://github.com/eriicafes/hollywood-di/issues"
  },
  "homepage": "https://github.com/eriicafes/hollywood-di#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@vitest/coverage-v8": "^1.2.2",
    "shx": "^0.3.4",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^1.2.2"
  }
}